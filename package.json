  "license": "MIT",
  "private": "true",
  "scripts": {
    "start": "nodemon src/index.ts",
    "start:prod": "yarn migration:run && node src/index.ts",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/dataSource.config.ts",
    "migration:create": "yarn typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert",
    "prettify": "prettier --write src"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^17.4.4",
    "axios": "^1.3.4",
    "body-parser": "^1.19.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.2",
    "helmet": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.21",
    "pg": "^8.4.0",
    "prettier": "^2.8.4",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "swagger-jsdoc": "^6.2.8",
