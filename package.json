    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.5",
    "@types/pg": "^8.6.6",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "routing-controllers-openapi": "^4.0.0"
  }
